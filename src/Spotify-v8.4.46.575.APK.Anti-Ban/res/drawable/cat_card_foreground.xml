<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:state_enabled="false">
        <shape>
            <solid android:color="@color/cat_light_green_5" />
            <stroke android:width="2.0dip" android:color="@color/cat_light_green_40" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/cat_black_40" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/cat_black_20" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="@color/cat_light_green_20" />
            <stroke android:width="2.0dip" android:color="@color/cat_light_green" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/pasteTransparent" />
        </shape>
    </item>
</selector>